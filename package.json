{
  "name": "violet",
  "productName": "violet",
  "version": "0.0.1",
  "main": "main.js",
  "private": true,
  "scripts": {
    "serve": "node -r babel-register node_modules/webpack-dev-server/bin/webpack-dev-server --config=webpack.config.development.js --port=3000",
    "start": "cross-env HOT=1 NODE_ENV=development electron -r babel-register ./electron/main.js",
    "start-pro": "cross-env NODE_ENV=production electron ./",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.main.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.js --progress --profile --colors",
    "pack": "cross-env NODE_ENV=production node -r babel-register package.js",
    "pack-all": "cross-env NODE_ENV=production node -r babel-register package.js --all"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/simongfxu/violet.git"
  },
  "keywords": [
    "electron",
    "markdown",
    "violet",
    "zhihu",
    "github",
    "medium",
    "react",
    "redux"
  ],
  "author": "simonxu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/simongfxu/violet/issues"
  },
  "homepage": "https://github.com/simongfxu/violet#readme",
  "description": "",
  "devDependencies": {
    "asar": "^0.11.0",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-webpack-loaders": "^0.4.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.7.2",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "electron-debug": "latest",
    "electron-packager": "latest",
    "electron-prebuilt": "latest",
    "electron-rebuild": "latest",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^4.3.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
